<?xml version="1.0" encoding="utf-8"?>
<module description="SYS" id="SYS">
	<register acronym="CLKCNTL" description="Clock Control Register" id="CLKCNTL" offset="0x00" width="32">
		<bitfield begin="31" description="RESERVED" end="10" id="RESERVED" rwaccess="" width="22">
		</bitfield>
		<bitfield begin="9" description="MCLK (processor clock) divide ratio" end="8" id="M_DIV_RATIO" rwaccess="RW" width="2">
		</bitfield>
		<bitfield begin="7" description="RESERVED" end="7" id="RESERVED" rwaccess="" width="1">
		</bitfield>
		<bitfield begin="6" description="CLKOUT pin source" end="5" id="CLKSR" rwaccess="RW" width="2">
		</bitfield>
		<bitfield begin="4" description="RESERVED" end="4" id="RESERVED" rwaccess="" width="1">
		</bitfield>
		<bitfield begin="3" description="CLKOUT pin output data" end="3" id="CLKDOUT" rwaccess="RW" width="1">
		</bitfield>
		<bitfield begin="2" description="RESERVED" end="2" id="RESERVED" rwaccess="" width="1">
		</bitfield>
		<bitfield begin="1" description="ARM Mode" end="0" id="ARM_MODE" rwaccess="RW" width="2">
		</bitfield>
	</register>
	<register acronym="SYSECR" description="System Exception Control Register" id="SYSECR" offset="0x10" width="32">
		<bitfield begin="31" description="RESERVED" end="16" id="RESERVED" rwaccess="" width="16">
		</bitfield>
		<bitfield begin="15" description="Software reset" end="14" id="RESET" rwaccess="RW" width="2">
		</bitfield>
		<bitfield begin="13" description="RESERVED" end="3" id="RESERVED" rwaccess="" width="11">
		</bitfield>
		<bitfield begin="2" description="Peripheral access violation override" end="2" id="PACCOVR" rwaccess="RW" width="1">
		</bitfield>
		<bitfield begin="1" description="Memory access reset override" end="1" id="ACCOVR" rwaccess="RW" width="1">
		</bitfield>
		<bitfield begin="0" description="Illegal address reset override" end="0" id="ILLOVR" rwaccess="RW" width="1">
		</bitfield>
	</register>
	<register acronym="SYSESR" description="System Exception Status Register" id="SYSESR" offset="0x14" width="32">
		<bitfield begin="31" description="RESERVED" end="16" id="RESERVED" rwaccess="" width="16">
		</bitfield>
		<bitfield begin="15" description="Power-up reset flag" end="15" id="PORRST" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="14" description="Clock fail flag" end="14" id="CLKRST" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="13" description="Watchdog reset flag" end="13" id="WDRST" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="12" description="Illegal mode flag" end="12" id="ILLMODE" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="11" description="Illegal address access flag" end="11" id="ILLADR" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="10" description="Illegal memory access flag" end="10" id="ILLACC" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="9" description="Peripheral illegal access flag" end="9" id="PILLACC" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="8" description="Illegal address map flag" end="8" id="ILLMAP" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="7" description="Software reset flag" end="7" id="SWRST" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="6" description="RESERVED" end="0" id="RESERVED" rwaccess="" width="7">
		</bitfield>
	</register>
	<register acronym="ABRTESR" description="Abort Exception Status Register" id="ABRTESR" offset="0x18" width="32">
		<bitfield begin="31" description="RESERVED" end="16" id="RESERVED" rwaccess="" width="16">
		</bitfield>
		<bitfield begin="15" description="Illegal address abort" end="15" id="ADRABT" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="14" description="Memory access abort" end="14" id="MEMABT" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="13" description="Peripheral access violation error" end="13" id="PACCVIO" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="12" description="RESERVED" end="0" id="RESERVED" rwaccess="" width="13">
		</bitfield>
	</register>
	<register acronym="GLBSTAT" description="Global Status Register" id="GLBSTAT" offset="0x1C" width="32">
		<bitfield begin="31" description="RESERVED" end="8" id="RESERVED" rwaccess="" width="24">
		</bitfield>
		<bitfield begin="7" description="System illegal address flag" end="7" id="SYSADDR" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="6" description="System illegal access flag" end="6" id="SYSACC" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="5" description="MPU illegal address flag" end="5" id="MPUADDR" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="4" description="MPU illegal access flag" end="4" id="MPUACC" rwaccess="R" width="1">
		</bitfield>
		<bitfield begin="3" description="RESERVED" end="0" id="RESERVED" rwaccess="" width="4">
		</bitfield>
	</register>
	<register acronym="DEV" description="Device Identification Register" id="DEV" offset="0x20" width="32">
		<bitfield begin="31" description="RESERVED" end="16" id="RESERVED" rwaccess="" width="16">
		</bitfield>
		<bitfield begin="15" description="Device identification code" end="0" id="DEV" rwaccess="R" width="16">
		</bitfield>
	</register>
	<register acronym="SSIF" description="System Software Interrupt Flag Register" id="SSIF" offset="0x28" width="32">
		<bitfield begin="31" description="RESERVED" end="1" id="RESERVED" rwaccess="" width="31">
		</bitfield>
		<bitfield begin="0" description="System software interrupt flag" end="0" id="SSIFLAG" rwaccess="R" width="1">
		</bitfield>
	</register>
	<register acronym="SSIR" description="System Software Interrupt Request Register" id="SSIR" offset="0x2C" width="32">
		<bitfield begin="31" description="RESERVED" end="16" id="RESERVED" rwaccess="" width="16">
		</bitfield>
		<bitfield begin="15" description="System software interrupt request key" end="8" id="SSKEY" rwaccess="RW" width="8">
		</bitfield>
		<bitfield begin="7" description="System software interrupt data" end="0" id="SSDATA" rwaccess="RW" width="8">
		</bitfield>
	</register>
</module>