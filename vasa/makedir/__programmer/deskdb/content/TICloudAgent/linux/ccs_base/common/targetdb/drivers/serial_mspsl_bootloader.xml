<?xml version="1.0"?>
<drivers id="CMCU_Bootloader">
	<!-- 
	isa Type="MSP430Xv2_BSL" will be used for all devices starts with:
	MSP430F5
	MSP430F6
	MS430FR
	-->
	<isa Type="MSP430Xv2_BSL" ProcID="0x6b800000">
        <driver file="..\..\DebugServer\bin\libDummyGTIDriver.so">
            <connectionType Type="SerialConnection"/>
			<property id="TargetAccess" Type="hiddenfield" Value="Flash Only" />
        </driver>
    </isa>
	
	<!-- 
	isa Type="CORTEX_M4_BSL" will be used for all devices starts with:
	MSP432E4
	-->	
	<isa Type="CORTEX_M4_BSL" ProcID="0x75803800">
