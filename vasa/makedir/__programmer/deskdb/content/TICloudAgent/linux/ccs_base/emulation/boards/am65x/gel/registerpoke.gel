menuitem "read-write register"

dialog read_32b_value_32b_address(address "Address: " )
{
        unsigned int * p_address;
        p_address = (unsigned int *) address;

        GEL_TextOut("Address %x:      %x\n",,,,,
        address,
        *p_address) ;
}

dialog write_32b_value_32b_address(address "Address: ", value "Value: " )
{
        unsigned int * p_address;
        p_address = (unsigned int *) address;
        *p_address = value;
        GEL_TextOut("writing %x to address: %x\n",,,,,
        value,
        address) ;
}

