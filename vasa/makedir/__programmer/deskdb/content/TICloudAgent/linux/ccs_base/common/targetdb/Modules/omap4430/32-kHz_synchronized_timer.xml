<module name="32KTimer" acronym="" XML_version="1.0" HW_revision="n/a" description="">
  <register id="_32KSYNCNT_REV" acronym="32KSYNCNT_REV" offset="0x0" width="32" description="This register contains the sync counter IP revision code.">
    <bitfield id="REVISION" width="32" begin="31" end="0" resetval="0x-" description="IP Revision" range="" rwaccess="R"/>
  </register>
  <register id="_32KSYNCNT_SYSCONFIG" acronym="32KSYNCNT_SYSCONFIG" offset="0x4" width="32" description="This register is used for IDLE modes only.">
    <bitfield id="Reserved" width="27" begin="31" end="5" resetval="0x0" description="Reads return 0." range="" rwaccess="R"/>
    <bitfield id="IDLEMODE" width="2" begin="4" end="3" resetval="0x0" description="Power management REQ/ACK control" range="" rwaccess="RW"/>
    <bitfield id="Reserved" width="3" begin="2" end="0" resetval="0x0" description="Reads return 0." range="" rwaccess="R"/>
  </register>
  <register id="_32KSYNCNT_CR" acronym="32KSYNCNT_CR" offset="0x10" width="32" description="This register contains the 32-kHz sync counter value.">
    <bitfield id="COUNTER_VALUE" width="32" begin="31" end="0" resetval="0x00000003" description="Counter register value" range="" rwaccess="R"/>
  </register>
</module>
