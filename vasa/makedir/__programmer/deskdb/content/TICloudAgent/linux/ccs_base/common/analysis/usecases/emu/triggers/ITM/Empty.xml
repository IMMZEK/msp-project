<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<breakpoints xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="breakpoints.xsd">

  <breakpoint type="AET">

    <property>
      <name>Hardware Configuration</name>
      <property>
        <name>Type</name>
        <string>Trace</string>
        <property>
            <name>Triggers</name>
            <string>ITM Trace</string>
            <property>
              <name>Timestamping</name>
              <property>
                <name>Local Timestamp Enable</name>
                <boolean>1</boolean>
                <property>
                  <name>Local Timestamp Prescaler</name>
                  <string>No prescaling</string>
                </property>
              </property>
            </property>
        </property>
        <property>
          <name>Actions</name>
          <string>Trace On</string>
          <property>
            <name>What to Trace</name>
            <string>(Select data to trace)</string>
            <property>
              <name>Program Address</name>
              <boolean>0</boolean>
            </property>
          </property>
        </property>
      </property>
    </property>

    <property>
      <name>Miscellaneous</name>
      <property>
        <name>Group</name>
        <string>Default Group</string>
      </property>
      <property>
        <name>Name</name>
        <string>ITM SW Messages</string>
      </property>
    </property>
    <enabled>0</enabled>

  </breakpoint>

</breakpoints>
