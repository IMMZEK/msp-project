	<property id="FlashEraseToggle0Button">
		<target>TMS320C28XX</target>
		<name>Toggle Bank 0 Selection</name>
		<valueType>Button</valueType>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
		<action>for( var i = 8; DPActiveDebugSession.options.optionExist( "FlashC28Bank0Sector" + i ); ++i ) { DPActiveDebugSession.options.setBoolean( "FlashC28Bank0Sector" + i, !DPActiveDebugSession.options.getBoolean( "FlashC28Bank0Sector" + i ) ); }</action>
	</property>
	
