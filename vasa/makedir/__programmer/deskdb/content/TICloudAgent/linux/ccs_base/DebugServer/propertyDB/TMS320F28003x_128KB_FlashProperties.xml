	<!-- Properties -->
	<property id="FlashOSCCLK">
		<target>TMS320C28XX</target>
		<name>OSCCLK (MHz)(INTOSC2)</name>
		<valueType>Numeric</valueType>
		<defaultValue>10</defaultValue>
		<readOnly>1</readOnly>
	</property>
	
	<property id="FlashCPUCLK">
		<target>TMS320C28XX</target>
		<name>CPUCLK (MHz)</name>
		<valueType>Numeric</valueType>
		<defaultValue>120</defaultValue>
		<readOnly>1</readOnly>
	</property>
	
	<property id="FlashAutoECCSetting">
		<target>TMS320C28XX</target>
		<name>Auto ECC Generation</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="FlashRangeText">
		<target>TMS320C28XX</target>
		<name>If range option is enabled, the given range(s) will be avoided during Flash programming.
Note: Please list multiple ranges in ascending order separated by a comma</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashRangeToggle">
		<target>TMS320C28XX</target>
		<name>Enable Range Avoidance</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashRangeAddr">
		<target>TMS320C28XX</target>
		<name>Range: </name>
		<valueType>String</valueType>
		<defaultValue>0x80000-0x81FFF</defaultValue>
		<enableIf>
			<property id="FlashRangeToggle">1</property>
		</enableIf>
	</property>
	
	<property id="FlashCombineText">
		<target>TMS320C28XX</target>
		<name>Note: any unwritten memory locations between sections will be filled with 1 when sections are combined,
and ECC will be programmed if AutoEccGeneration is enabled.</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashCombineText2">
		<target>TMS320C28XX</target>
		<name>Note2: Program sections on different pages will not be combined. If AutoEccGeneration is enabled, 
page 0 and page 1 sections cannot share the same 64-bit aligned memory.</name>
		<valueType>Text</valueType>
	</property>
	
	<property id="FlashAlignCombineToggle">
		<target>TMS320C28XX</target>
		<name>Combine Sections during Program Load to increase performance</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashAlignmentSize">
		<target>TMS320C28XX</target>
		<name>Specify maximum size for combining sections: 0x</name>
		<valueType>String</valueType>
		<defaultValue>1000</defaultValue>
		<disableIf>
			<property id="FlashCombineSections">0</property>
		</disableIf>
		<hidden />
	</property>
	
	<property id="FlashResetOnOperation">
		<target>generic</target>
		<name>Reset target before flash programming/operations</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="FlashDownloadSetting">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Erase and Program</value>
			<value>Program Only</value>
			<value>Verify Only</value>
		</values>
		<defaultValue>Erase and Program</defaultValue>
	</property>
	
	<property id="FlashBlankCheckToggle">
		<target>TMS320C28XX</target>
		<name>Perform Blank Check before loading data to Flash memory</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="FlashVerifySetting">
		<target>TMS320C28XX</target>
		<name>Verify Flash after Program</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="FlashEraseSelection">
		<target>TMS320C28XX</target>
		<name></name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Entire Flash</value>
			<value>Necessary Sectors Only (for Program Load)</value>
			<value>Selected Sectors Only</value>
		</values>
		<defaultValue>Entire Flash</defaultValue>
	</property>
	
	<property id="FlashC28Bank0Sector8">
		<target>TMS320C28XX</target>
		<name>Sector 8 (0x88000 - 0x88FFF)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<enableIf>
			<property id="FlashEraseSelection">Selected Sectors Only</property>
		</enableIf>
	</property>
	
