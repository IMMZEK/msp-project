    <bitenum description="Normal mode" id="en_1_0x0" token="en_1_0x0" value="0x0"></bitenum>
  </bitfield>
    
  <bitfield begin="0" description=" Internal OCP clock gating strategy" end="0" id="AUTOIDLE" rwaccess="RW" width="1">
