<?xml version="1.0" encoding="utf-8"?>
<newBreakpoint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="schema.xsd" name="VM Breakpoint" breakpointType="1">
  <targets>
    <target>
      <type>dsk,emu</type>
      <familyId>470</familyId>
      <subfamilyId>53</subfamilyId>
      <revision>*</revision>
    </target>
  </targets>
  <defaultProperty>
    <property name="Miscellaneous.Name" value="VM Breakpoint" />
    <property name="Hardware Configuration.Type" value="VM Breakpoint"></property>
  </defaultProperty>
  <submitSequence>
    <id>Hardware Configuration.Type.Virtual Machine ID</id>
  </submitSequence>
  <showIn>
    <breakpointView autoSubmit="false" />
  </showIn>
  <dialog colCount="2">
    <label id="label_location" widthHint="50" heightHint="18" verticalAlign="center" horizontalAlign="fill">
      <text>Virtual Machine ID:</text>
    </label>
    <textBox id="Hardware Configuration.Type.Virtual Machine ID" widthHint="190" heightHint="20" verticalAlign="center" horizontalAlign="center">
      <text>Enter Virtual Machine ID here...</text>
    </textBox>
  </dialog>
</newBreakpoint>
