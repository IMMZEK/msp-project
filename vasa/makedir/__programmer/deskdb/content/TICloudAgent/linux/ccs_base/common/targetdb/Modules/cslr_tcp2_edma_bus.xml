         <bitenum id="YES" value="1" token="YES" description="Interleaver table is sent to the TCP (required for SA mode)" />
      </bitfield>
      <bitfield id="_RESV" width="1" begin="11" end="11" resetval="0" description="Reserved" range="" rwaccess="N">
         
      </bitfield>
