StartUp()
{    
    /* Load Common Gel Files */
    GEL_LoadGel("$(GEL_file_dir)/am35x_emif_config.gel");
    GEL_LoadGel("$(GEL_file_dir)/ES2_prcm_clock_config.gel");
    GEL_LoadGel("$(GEL_file_dir)/omap3430_cortexA_utils.gel");
    GEL_LoadGel("$(GEL_file_dir)/reconfigure_firewalls.gel");
    GEL_LoadGel("$(GEL_file_dir)/sDMA_Access.gel");
    GEL_LoadGel("$(GEL_file_dir)/ES2_sdram_config.gel");
} 
  

OnTargetConnect()
{
    startup_sequence();
}

hotmenu L4Setup()
{
	GEL_TextOut("Starting L4 Setup\n");
	/* L4 - 0x48000000 */
    *(int*)0x5D000050 = 0x00000030;
    *(int*)0x5D000058 = 0x4800000C;
    *(int*)0x5D00005C = 0x48000140;
    *(int*)0x5D000054 = 0x00000001;

    /* L4 Wakeup - 0x49000000 */
    *(int*)0x5D000050 = 0x00000040;
    *(int*)0x5D000058 = 0x4900000C;
    *(int*)0x5D00005C = 0x49000140;
    *(int*)0x5D000054 = 0x00000001;
	GEL_TextOut("Finished L4 Setup\n");
}

OnReset()
{
    Watchdog_disable();
}

startup_sequence()
{
   
   Watchdog_disable();
   SelectSysClock_26MHz();
   Setup_ClockConfig_IIA();  /* Not needed in QT */
   Setup_MPUConfig_OPP3();
   GEL_MapOff();
   GEL_MapReset();
   memorymap_init();
   GEL_MapOn(); 
	/* CLKOUT1 Pad conf*/
	* 0x48002A18 &= 0xFFF8FFFF;
	/* CLKOUT2 ENABLE */
	* 0x48004D70 |= 0x81;
	/* CLKOUT2 Pad conf*/
	* 0x480021E0 &= 0xFFF8FFFF;
  DDR2_micron_166MHz();
}


hotmenu Watchdog_disable()
{
   /* Enable clock */
   /* enable Interface clock */
   *(int*)0x48004C10 = 0x20;

   /* enable functional clock */
   *(int*)0x48004C00 = 0x20;

   /* Check that module is Idle */
   while ( ((*(int *)0x48004C20) & 0x20));
       
   /* Disabler  watchdog 2 */
   /* Wait until reset complete */
   while (!((*(int*)0x48314014) & 0x01));

   /* Disable 32Khz watchdog timer */
   *(int*)0x48314048 = 0x0000AAAA;

   while ( ((*(int *)0x48314034) & 0x10));

   /* Disable 32Khz watchdog timer */
   *(int*)0x48314048 = 0x00005555;

   while ( ((*(int *)0x48314034) & 0x10));

   GEL_TextOut("OMAP 32K Watchdog Timer is disable\n");
}

