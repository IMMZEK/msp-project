<?xml version="1.0" encoding="UTF-8" standalone="yes"?>


<module description="SYNCTIMER" id="SYNCTIMER">
  
  
  <register acronym="SYNCTIMER32K_SYNCNT_REV" description="" id="SYNCTIMER32K_SYNCNT_REV" offset="0x0" width="32">
    
  <bitfield begin="31" description=" " end="8" id="RESERVED_1" rwaccess="R" width="24"></bitfield>
    
  <bitfield begin="7" description=" Revision number." end="0" id="CID_REV" rwaccess="R" width="8"></bitfield>
  </register>
  
  
  <register acronym="SYNCTIMER32K_SYSCONFIG" description="" id="SYNCTIMER32K_SYSCONFIG" offset="0x4" width="32">
    
  <bitfield begin="31" description=" " end="5" id="RESERVED_1" rwaccess="RW" width="27"></bitfield>
    
  <bitfield begin="4" description=" Power Management Req/Ack Control." end="3" id="IDLEMODE" rwaccess="RW" width="2">
    <bitenum description="Reserved." id="en_4_0x3" token="en_4_0x3" value="0x3"></bitenum>
    <bitenum description="Reserved." id="en_3_0x2" token="en_3_0x2" value="0x2"></bitenum>
    <bitenum description="No-idle. An idle request is never acknowledged." id="en_2_0x1" token="en_2_0x1" value="0x1"></bitenum>
    <bitenum description="Force idle. An idle request is acknowledged unconditionally." id="en_1_0x0" token="en_1_0x0" value="0x0"></bitenum>
  </bitfield>
    
  <bitfield begin="2" description=" " end="0" id="RESERVED_2" rwaccess="RW" width="3"></bitfield>
  </register>
  
  
  <register acronym="SYNCTIMER32K_CR" description="" id="SYNCTIMER32K_CR" offset="0x10" width="32">
    
  <bitfield begin="31" description=" Read counter high. [[br]]Value of [[br]]32-kHz SYNCH counter (16 bit LSB)." end="16" id="CTR_HI" rwaccess="R" width="16"></bitfield>
    
  <bitfield begin="15" description=" Read counter low. [[br]]Value of [[br]]32-kHz SYNCH counter (16 bit MSB)." end="0" id="CTR_LO" rwaccess="R" width="16"></bitfield>
  </register>
</module>
