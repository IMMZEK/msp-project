<?xml version="1.0"?>
<!-- This file defines a job the AET can support for a specific ISA. -->

<!--=====================================================================-->
<!-- Same as AETT_property_ks3_system_trace_am64x.xml but no            -->
<!-- A53 CPtracer2 probes                                                 -->
<!--=====================================================================-->

<?CTOOLS version="1.1.0.0" Copyright="Copyright (c) 2020 Texas Instruments"?>
<AET>
    <JOB_MAJOR_V>1</JOB_MAJOR_V>
    <JOB_MINOR_V>0</JOB_MINOR_V>

	<PROPERTY_STRING>                                           
        <NAME>Type</NAME>                                         
        <DESCRIPTION>Display all the AET functions</DESCRIPTION>	
        <ALLOWEDLIST>
            <ALLOWEDITEM>
                <ALLOWED_KEY>TRACE</ALLOWED_KEY>
                <ALLOWED_STRING>Trace</ALLOWED_STRING>
            </ALLOWEDITEM>
        </ALLOWEDLIST>
    </PROPERTY_STRING>
    
	<PROPERTY_STRING>
	  <NAME>System Trace Type</NAME>	
 	  <DESCRIPTION>Specify the type of system trace</DESCRIPTION>	
	  <KEYITEM>
		  <KEYNAME>TRACE</KEYNAME>	
		  <KEYNUMBER>1</KEYNUMBER>	
	  </KEYITEM>
	  <ALLOWEDLIST>
		  <ALLOWEDITEM>
			<ALLOWED_KEY>STM SWMSG</ALLOWED_KEY>
			<ALLOWED_ENUM>0xC000000A</ALLOWED_ENUM>
			<ALLOWED_STRING>STM Software Messages</ALLOWED_STRING>
		  </ALLOWEDITEM>
		  <ALLOWEDITEM>
			<ALLOWED_KEY>NAVSS TRACE</ALLOWED_KEY>
			<ALLOWED_ENUM>0xC000000D</ALLOWED_ENUM>
			<ALLOWED_STRING>NAVSS Trace</ALLOWED_STRING>
		  </ALLOWEDITEM>
	  </ALLOWEDLIST>
	</PROPERTY_STRING>
    
    <!--=====================================================================-->
    <!--                 Begin STM Software Messages use case                -->
    <!--=====================================================================-->
    
	<PROPERTY_STRING>                                           
		<NAME>Master select</NAME>                                         
		<DESCRIPTION>Select the masters.</DESCRIPTION>	
		<KEYITEM>
			<KEYNAME>STM SWMSG</KEYNAME>	
			<KEYNUMBER>1</KEYNUMBER>	
		</KEYITEM>
        <ALLOWEDLIST>
		  <ALLOWEDITEM>
			<ALLOWED_KEY>MASTER ALL</ALLOWED_KEY>
			<ALLOWED_ENUM>0x81000000</ALLOWED_ENUM>
			<ALLOWED_STRING>All</ALLOWED_STRING>
		  </ALLOWEDITEM>
	  </ALLOWEDLIST>
	</PROPERTY_STRING>
    
    
    <!--=====================================================================-->
    <!--                 End STM Software Messages use case                  -->
    <!--=====================================================================-->
    
    
    
    <!--=====================================================================-->
    <!--               Begin Ring Accelerator trace use case                 -->
    <!--=====================================================================-->
    
  
     
	<PROPERTY_BOOL>
		<NAME>Debugger Enabled Trace</NAME>	
		<DESCRIPTION>If true, the debugger will program the RA GCFG registers, aggregator, and trace sink. If false, the debugger will only program the aggregator and trace sink.</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>1</KEYNUMBER>	
		</KEYITEM>
		<VALUE>none</VALUE>
		<ENUMVALUE>0x80860000</ENUMVALUE>
	</PROPERTY_BOOL>
    
    <PROPERTY_BOOL>
		<NAME>All Queues</NAME>	
		<DESCRIPTION>If true, all queues will be traced. If false, only selected queue will be traced.</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>2</KEYNUMBER>	
		</KEYITEM>
		<VALUE>true</VALUE>
		<ENUMVALUE>0x80860001</ENUMVALUE>
	</PROPERTY_BOOL>
    
    <PROPERTY_INTEGER>
		<NAME>Queue number</NAME>
		<DESCRIPTION>Specify the queue number to trace (0 - 1024)</DESCRIPTION>
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>3</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0</VALUE>
		<DISPLAYTYPE>DEC</DISPLAYTYPE>
		<RANGELO>0</RANGELO>
		<RANGEHI>1024</RANGEHI>
		<ENUMVALUE>0x80860002</ENUMVALUE>
	</PROPERTY_INTEGER>
   
    <PROPERTY_BOOL>
		<NAME>Message Data</NAME>	
		<DESCRIPTION>If true, message data will be traced. If false, only the operation will be traced.</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>4</KEYNUMBER>	
		</KEYITEM>
		<VALUE>false</VALUE>
		<ENUMVALUE>0x80860003</ENUMVALUE>
	</PROPERTY_BOOL>
    
    <PROPERTY_INTEGER>
		<NAME>Overflow Queue</NAME>
		<DESCRIPTION>Specify the queue number to send overflow messages to (0 - 1024)</DESCRIPTION>
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>4</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0x0</VALUE>
		<DISPLAYTYPE>DEC</DISPLAYTYPE>
		<RANGELO>0</RANGELO>
		<RANGEHI>1024</RANGEHI>
		<ENUMVALUE>0x80860004</ENUMVALUE>
	</PROPERTY_INTEGER>
    
    <PROPERTY_INTEGER>
		<NAME>Event ID</NAME>
		<DESCRIPTION>Specify the event ID to send when detecting a bus error (0 - 65535)</DESCRIPTION>
		<KEYITEM>
			<KEYNAME>RA LEVEL</KEYNAME>	
			<KEYNUMBER>5</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0</VALUE>
		<DISPLAYTYPE>DEC</DISPLAYTYPE>
		<RANGELO>0</RANGELO>
		<RANGEHI>65535</RANGEHI>
		<ENUMVALUE>0x80860005</ENUMVALUE>
	</PROPERTY_INTEGER>
    
    <!--=====================================================================-->
    <!--                 End Ring Accelerator trace use case                 -->
    <!--=====================================================================-->
    
    <!--=====================================================================-->
    <!--                      Begin shared properties                        -->
    <!--=====================================================================-->
    
	<PROPERTY_STRING>
		<NAME>Display Settings</NAME>	
		<DESCRIPTION>Input information for trace display</DESCRIPTION>	
		<KEYITEM>
			<KEYNAME>STM SWMSG</KEYNAME>	
			<KEYNUMBER>1</KEYNUMBER>	
		</KEYITEM>
		<KEYITEM>
			<KEYNAME>PROBE SOC</KEYNAME>	
			<KEYNUMBER>3</KEYNUMBER>	
		</KEYITEM>
		<KEYITEM>
			<KEYNAME>NAVSS SOC</KEYNAME>	
			<KEYNUMBER>5</KEYNUMBER>	
		</KEYITEM>
		<CHILDKEY>DISP OP</CHILDKEY>	
		<EDITABLE>FALSE</EDITABLE>	
	</PROPERTY_STRING>

	<PROPERTY_INTEGER>
		<NAME>Clock Rate(Mhz)</NAME>	
		<DESCRIPTION>The clock rate for converting clock cycles to seconds</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>DISP OP</KEYNAME>	
			<KEYNUMBER>1</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0xC8</VALUE>
		<DISPLAYTYPE>HEX</DISPLAYTYPE>
		<ENUMVALUE>0x80090000</ENUMVALUE>
	</PROPERTY_INTEGER>

	<PROPERTY_BOOL>
		<NAME>Suppress Zeros</NAME>	
		<DESCRIPTION>Suppress zeros data values in decoded trace data.</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>DISP OP</KEYNAME>	
			<KEYNUMBER>2</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0</VALUE>
		<ENUMVALUE>0x80090002</ENUMVALUE>
	</PROPERTY_BOOL>
    
	<PROPERTY_STRING>
		<NAME>Sync Core Halt</NAME>	
		<DESCRIPTION>Sync trace halt capture to a core</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>DISP OP</KEYNAME>	
			<KEYNUMBER>3</KEYNUMBER>	
		</KEYITEM>
		<VALUE>none</VALUE>
		<ENUMVALUE>0x80090003</ENUMVALUE>
	</PROPERTY_STRING>
    
	<PROPERTY_INTEGER>
        <NAME>SOC Probe Bus Clock</NAME>	
        <DESCRIPTION>Clock rate of SOC bus probes in MHz. Calculated from MAIN_SYSCLK0 / 2.</DESCRIPTION>		
		<KEYITEM>
			<KEYNAME>PROBE SOC</KEYNAME>	
			<KEYNUMBER>3</KEYNUMBER>	
		</KEYITEM>
		<VALUE>0xFA</VALUE>
		<DISPLAYTYPE>HEX</DISPLAYTYPE>
		<ENUMVALUE>0x80090004</ENUMVALUE>
	</PROPERTY_INTEGER>
    
    <!--=====================================================================-->
    <!--                        End shared properties                        -->
    <!--=====================================================================-->
		
</AET>
